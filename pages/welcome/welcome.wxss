/* pages/welcome/welcome.wxss */
.m-header {
  width: 100%;
  height: 756rpx;
}

input {
  box-sizing: border-box;
  padding-left: 30rpx;
  font-family: monospace
}

.m-header image {
  width: 100%;
  height: 100%;
}

.title {
  width: 420rpx;
  height: 112rpx;
  margin: 0 auto;
  margin-top: 32rpx;
}
.title image{
  width: 100%;
  height: 100%;
}

.m-infoInput {
  width: 608rpx;
  margin: 0 auto;
  margin-top: 30rpx;
}

.name {
  width: 240rpx;
  height: 76rpx;
  background: rgba(255, 255, 255, 1);
  border: 1.5px solid rgba(19, 22, 38, 1);
  opacity: 1;
  margin-bottom: 32rpx;
}

.name2 {
  width: 335rpx;
  height: 76rpx;
  background: rgba(255, 255, 255, 1);
  border: 1.5px solid rgba(19, 22, 38, 1);
  opacity: 1;
  margin-bottom: 32rpx;
}

.code {
  width: 614rpx;
  height: 76rpx;
  background: rgba(255, 255, 255, 1);
  border: 1.5px solid rgba(19, 22, 38, 1);
  opacity: 1;
  margin-bottom: 32rpx;
}

.m-name {
  display: flex;
  justify-content: space-between;
  width: 614rpx;
}

.inputBtn {
  width: 614rpx;
  background: rgba(19, 22, 38, 1);
  opacity: 1;
  font-size: 34rpx;
  font-family: monospace;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  opacity: 1;
  text-align: center;
}

button:not([size='mini']) {
