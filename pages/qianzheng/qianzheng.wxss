.qianzhenImg {
  width: 668rpx;
  height: 430rpx;
  background: rgba(225, 233, 244, 0.8);
  opacity: 1;
  border-radius: 20rpx;
  overflow: hidden;
  margin: 0 auto;
}

.qianzhenImg image {
  width: 100%;
  height: 100%;
  background-position: top;
  background-repeat: no-repeat;
  background-size: cover;
}

.saveImg {
  width: 176rpx;
  line-height: 64rpx;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(118, 147, 191, 1);
  opacity: 1;
  border-radius: 32rpx;
  color: rgba(118, 147, 191, 1);
  text-align: center;
  margin: 0 auto;
  margin-top: 66rpx;
}